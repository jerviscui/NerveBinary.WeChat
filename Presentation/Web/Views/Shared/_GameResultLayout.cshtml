@model Web.Models.Game.SubjectResultModel

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/common_mobile.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.2.0.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1, maximum-scale=1, user-scalable=no" />
    <style>
        #mcover {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.9);
            display: none;
            z-index: 20000;
        }

            #mcover img {
                position: fixed;
                right: 0;
                top: 0;
                width: 100%;
                height: auto;
                z-index: 20001;
            }
    </style>
</head>

<body app-name="test" app-mod="3">
    <div class="wnl-test-name">
        <div class="i-inner">
            @RenderBody()
            <ul class="i-result-btn clearfix">
                <li><a href="@Url.Action("Detail", "Game", new { subjectId = Model.SubjectId })" class="i-reset" stat="NameTestResultRe">我也要玩</a></li>
                @*<li><a href="@Url.Action("Index", "Game")" class="i-share" stat="NameTestShare">更多好玩</a></li>*@

                <li><a id="do" href="javascript:void(0);" class="i-share" stat="NameTestShare">分享出去</a></li>
            </ul>
        </div>
    </div>

    <div id="mcover" onclick="$(this).hide()">
        <img src="~/Images/guide.png" />
    </div>
    <script type="text/javascript">
        $(function () {
            $("#do").click(function () {
                $('#mcover').show();
                return false;
            });
        });
    </script>
</body>
</html>
